{
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "bracketSameLine": false,
    "semi": true,
    "jsxBracketSameLine": false,
    "importOrder": ["^react$", "^@?\\w", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "proseWrap": "always",
    "endOfLine": "lf"
}
